@model IEnumerable<RentalSystem.Models.ProductModel>

@{
    ViewBag.Title = "MyOrders";
}

@Html.Partial("_navbar")
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.ProductName)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Image1)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
           @Html.ActionLink(item.ProductName, "ProductDetails", "Product",new { prodId = Convert.ToInt32(item.ProductId)},null)
        </td>
        
        <td>
            <img src=@Url.Content(item.Image1) width="220" height="150" />
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rent)
        </td>
       
    </tr>
}

</table>
